<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

	<property name="pi" value="3.14157"/>

	<xacro:macro name="motoman_mh50" params="prefix">
		<!-- link list -->
		<link name="${prefix}base_link">
			<visual>
			        <origin rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/visual/MH50_BASE_AXIS.dae" />
				</geometry>
				<material name="blue">
					<color rgba="0.8 0.8 .8 1"/>
				</material>
			</visual>
			<collision>
			        <origin rpy="0 0 0" xyz="-0.086 0 0.1295"/>
				<geometry>
					<box size="0.635 0.470 0.259"/>
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_s">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/visual/MH50_S_AXIS.dae"/>
				</geometry>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/collision/MH50_S_AXIS.dae" />
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_l">
			<visual>
				<!-- <origin rpy="1.57 3.1416 0" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/visual/MH50_L_AXIS.dae"/>
				</geometry>
			</visual>
			<collision>
				<!-- <origin rpy="1.57 3.1416 0" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/collision/MH50_L_AXIS.dae" />
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_u">
			<visual>
				<!-- <origin rpy="1.57 -3.1415 0" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/visual/MH50_U_AXIS.dae"/>
				</geometry>
			</visual>
			<collision>
				<!-- <origin rpy="1.57 -3.1415 0" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/collision/MH50_U_AXIS.dae" />
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_r">
			<visual>
				<!-- <origin rpy="0 0 3.1416" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/visual/MH50_R_AXIS.dae"/>
				</geometry>
			</visual>
			<collision>
				<!-- <origin rpy="0 0 3.1416" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/collision/MH50_R_AXIS.dae" />
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_b">
			<visual>
				<!-- <origin rpy="-1.57 0 3.1416" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/visual/MH50_B_AXIS.dae"/>
				</geometry>
				<material name="blue"/>
			</visual>
			<collision>
				<!-- <origin rpy="-1.57 0 3.1416" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/collision/MH50_B_AXIS.dae" />
				</geometry>
			</collision>
		</link>
		<link name="${prefix}link_t">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/visual/MH50_T_AXIS.dae" />
				</geometry>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh50_support/meshes/mh50/collision/MH50_T_AXIS.dae" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>
		<!-- end of link list -->
		<!-- joint list -->
		<joint name="${prefix}joint_s" type="revolute">
			<parent link="${prefix}base_link"/>
			<child link="${prefix}link_s"/>
			<origin xyz="0 0 0.259" rpy="0 0 0"/>
			<axis xyz="0 0 1" />
			<limit lower="-3.14159" upper="3.14159" effort="100" velocity="2.96" />
		</joint>
		<joint name="${prefix}joint_l" type="revolute">
			<parent link="${prefix}link_s"/>
			<child link="${prefix}link_l"/>
			<origin xyz="0.145 -0.1455 0.281" rpy="0 0 0"/>
			<axis xyz="0 1 0" />
			<limit lower="-1.5708" upper="2.35619" effort="100" velocity="2.96" />
		</joint>
		<joint name="${prefix}joint_u" type="revolute">
			<parent link="${prefix}link_l"/>
			<child link="${prefix}link_u"/>
			<origin xyz="-0.0 -0.0095 0.870" rpy="0 0 0"/>
			<axis xyz="0 -1 0" />
	      <!-- Old limits: lower="-2.96706" upper="4.38078" 
                   The new limits below were measured from an MH50F.  They ensure 
 		   that link_r won't hit link_l even when joint_r is at 45 degrees.  
		   The old limits were converted directly from the MH50 data sheet,
		   which was clearly way off. -->
			<limit lower="-1.55" upper="4.014" effort="100" velocity="2.96" />
		</joint>
		<joint name="${prefix}joint_r" type="revolute">
			<parent link="${prefix}link_u"/>
			<child link="${prefix}link_r"/>
			<origin xyz="0.306 0.155 0.210" rpy="0 0 0"/>
			<axis xyz="-1 0 0" />
			<limit lower="-6.28319" upper="6.28319" effort="100" velocity="3.48" />
		</joint>
		<joint name="${prefix}joint_b" type="revolute">
			<parent link="${prefix}link_r"/>
			<child link="${prefix}link_b"/>
			<origin xyz="0.7065 0 0" rpy="0 0 0"/>
			<axis xyz="0 -1 0" />
			<limit lower="-2.1817" upper="2.1817" effort="100" velocity="3.48" />
		</joint>
		<joint name="${prefix}joint_t" type="revolute">
			<parent link="${prefix}link_b"/>
			<child link="${prefix}link_t"/>
			<origin xyz="0.1595 0 0" rpy="0 0 0"/>
			<axis xyz="-1 0 0" />
			<limit lower="-6.2831" upper="6.2831" effort="100" velocity="6.97" />
		</joint>
		<!-- end of joint list -->
	   	<!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
	    	<link name="${prefix}tool0" />
	    	<joint name="${prefix}link_6-tool0" type="fixed">
	      		<origin xyz="0.0155 0 0" rpy="${pi} ${-pi/2.0} 0" />
	      		<parent link="${prefix}link_t" />
	      		<child link="${prefix}tool0" />
	    	</joint>
	</xacro:macro>
</robot>

